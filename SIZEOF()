#include <iostream>

using namespace std;

int main()
{
int a;
float b;
char c;
double d;
cout<<"SIZE"<<" "<<"int"<<" "<<"float"<<" "<<"char"<<" "<<"double"<<endl;
cout<<"      "<<sizeof(a)<<"    "<<sizeof(b)<<"     "<<sizeof(c)<<"     "<<sizeof(d)<<endl;
float x,y;
cin>>x>>y;
cout<<x*y;
return 0;
}
